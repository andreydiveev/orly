{
    "name": "cast/lets2fa-bot",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "telegram-bot/api": "^2.3",
        "guzzlehttp/guzzle": "^6.3",
        "sonata-project/google-authenticator": "^2.0",
        "laravel/lumen": "^6.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9",
        "heroku/heroku-buildpack-php": "^127.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/orlydev/heroku-nano"
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}
